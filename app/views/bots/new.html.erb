<%= form_for :bot, url: bots_path do |f| %>
  <p>
    <%= f.label :name, "選挙名" %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :url, "URL" %><br>
    <%= f.text_area :url, cols: 60, rows: 8 %>
  </p>
  
  <p>
    <%= f.label :article_id, "記事ID" %><br>
    <%= f.text_area :article_id, cols: 60 %>
  </p>
  
  <p>
    <%= f.label :date, "開票日" %><br>
    <%= f.text_area :date, cols: 60 %>
  </p>
  
  <p>
    <%= f.submit "登録" %>
  </p>
<% end %>